<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<GridPane fx:id="paneApproval" prefHeight="448.0" prefWidth="950.0" style="-fx-background-color: #ffff;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.ApprovalController">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="50.0" minHeight="50.0" prefHeight="50.0" vgrow="SOMETIMES" />
        <RowConstraints vgrow="SOMETIMES" />
    </rowConstraints>
    <children>
        <HBox nodeOrientation="RIGHT_TO_LEFT" prefHeight="43.0" />
        <TableView fx:id="table_approval" GridPane.rowIndex="1">
            <columns>
                <TableColumn fx:id="approval_id" maxWidth="49.0" minWidth="49.0" prefWidth="49.0" resizable="false" text="Mã số" />
                <TableColumn fx:id="approval_username" maxWidth="233.0" prefWidth="162.0" resizable="false" text="Username" />
                <TableColumn fx:id="approval_name" minWidth="0.0" prefWidth="203.0" resizable="false" text="Họ và tên" />
                <TableColumn fx:id="approval_cfr" prefWidth="313.0" resizable="false" text="Hội nghị" />
            </columns>
            <GridPane.margin>
                <Insets left="20.0" right="20.0" />
            </GridPane.margin>
        </TableView>
    </children>
</GridPane>
